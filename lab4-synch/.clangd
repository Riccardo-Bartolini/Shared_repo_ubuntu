CompileFlags:
  Add:
    - -ffreestanding
    - -mcpu=cortex-m3
    - -mthumb
    - -Wall
    - -Wextra
    - -Wshadow
    - -g3
    - -Os
    - -ffunction-sections
    - -fdata-sections
    - -I../FreeRTOS/FreeRTOS/Source/include
    - -I../FreeRTOS/FreeRTOS/Source/portable/GCC/ARM_CM3
    - -I.

LinkFlags:
  Add:
    - -T./mps2_m3.ld
    - -nostartfiles
    - -specs=nano.specs
    - -specs=nosys.specs
    - -Xlinker -Map=./Output/demo.map
    - -Xlinker --gc-sections
    - -mcpu=cortex-m3
    - -mthumb

